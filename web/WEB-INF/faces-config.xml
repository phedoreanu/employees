<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    <managed-bean>
        <managed-bean-name>abilitateBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.AbilitateBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>abilitateController</managed-bean-name>
        <managed-bean-class>com.adina.controller.AbilitateController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.UserBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>locMuncaAnteriorBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.LocMuncaAnteriorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>locMuncaAnteriorController</managed-bean-name>
        <managed-bean-class>com.adina.controller.LocMuncaAnteriorController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>


    <navigation-rule>
        <navigation-case>
            <from-outcome>adminList</from-outcome>
            <to-view-id>/admin/abilitate/abilitiesList.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterInsert</from-outcome>
            <to-view-id>/admin/abilitate/afterInsert.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
