<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    <managed-bean>
        <managed-bean-name>abilitateBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.AbilitateBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>abilitateController</managed-bean-name>
        <managed-bean-class>com.adina.controller.AbilitateController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>userBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.UserBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>userController</managed-bean-name>
        <managed-bean-class>com.adina.controller.UserController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>locMuncaAnteriorBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.LocMuncaAnteriorBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>locMuncaAnteriorController</managed-bean-name>
        <managed-bean-class>com.adina.controller.LocMuncaAnteriorController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>clasaConcediuBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.ClasaConcediuBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>clasaConcediuController</managed-bean-name>
        <managed-bean-class>com.adina.controller.ClasaConcediuController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>clasaSalariuBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.ClasaSalariuBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>clasaSalariuController</managed-bean-name>
        <managed-bean-class>com.adina.controller.ClasaSalariuController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>functieBean</managed-bean-name>
        <managed-bean-class>com.adina.bean.FunctieBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>functieController</managed-bean-name>
        <managed-bean-class>com.adina.controller.FunctieController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <navigation-case>
            <from-outcome>adminList</from-outcome>
            <to-view-id>/admin/abilitate/abilitiesList.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterInsert</from-outcome>
            <to-view-id>/admin/abilitate/afterInsert.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterWorkPlaceOperations</from-outcome>
            <to-view-id>/admin/previousWorkPlace/afterInsert.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterHolidayOperation</from-outcome>
            <to-view-id>/admin/holidayClass/afterOperations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterSalaryOperations</from-outcome>
            <to-view-id>/admin/salaryClass/afterOperations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterPositionOperations</from-outcome>
            <to-view-id>/admin/position/afterOperations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

     <navigation-rule>
        <navigation-case>
            <from-outcome>afterAccountOperations</from-outcome>
            <to-view-id>/admin/employeeAccount/afterOperations.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <navigation-case>
            <from-outcome>afterAdminUpdate</from-outcome>
            <to-view-id>/admin/index.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
